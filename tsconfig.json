{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Node",
		"strict": true,
		"jsx": "react",
		"esModuleInterop": true,
		"verbatimModuleSyntax": true
	},
	"include": [".astro/types.d.ts", "src/**/*"],
	"exclude": ["dist"]
}
